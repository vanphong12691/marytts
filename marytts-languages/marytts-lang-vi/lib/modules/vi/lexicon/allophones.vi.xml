    <consonant ph="x" ctype="f" cplace="v" cvox="-"/>
    <consonant ph="h" ctype="f" cplace="g" cvox="-"/>
    <consonant ph="ji" ctype="r" cplace="d" cvox="-"/>
    <consonant ph="w" ctype="r" cplace="d" cvox="+"/>
    <consonant ph="wu" ctype="r" cplace="d" cvox="+"/>
    <consonant ph="l" ctype="l" cplace="d" cvox="+"/>
