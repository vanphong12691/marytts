<allophones name="sampa" xml:lang="vi"
		features="vlng vheight vfront vrnd ctype cplace cvox">
<!-- Open source British English: Using US English CMUDict lexicon -->
	<silence ph="_"/>

	<vowel ph="u" vlng="l" vheight="1" vfront="3" vrnd="+"/>
	<vowel ph="W" vlng="l" vheight="1" vfront="3" vrnd="-"/>
