    <vowel ph="Os" vlng="s" vheight="2" vfront="3" vrnd="-"/>
	<vowel ph="i" vlng="l" vheight="1" vfront="1" vrnd="-"/>
	<vowel ph="e" vlng="l" vheight="2" vfront="2" vrnd="-"/>
    <vowel ph="E" vlng="l" vheight="2" vfront="1" vrnd="-"/>
    <vowel ph="Es" vlng="s" vheight="2" vfront="1" vrnd="-"/>
	<vowel ph="as" vlng="s" vheight="3" vfront="1" vrnd="-"/>
    <vowel ph="ie" vlng="d" vheight="1" vfront="2" vrnd="-"/>
	<vowel ph="uo" vlng="l" vheight="1" vfront="1" vrnd="+"/>
    <vowel ph="WoU" vlng="l" vheight="1" vfront="1" vrnd="+"/>
    <vowel ph="O" vlng="l" vheight="2" vfront="3" vrnd="-"/>
    <vowel ph="a" vlng="l" vheight="3" vfront="1" vrnd="-"/>
    <vowel ph="_1" vlng="l" vheight="1" vfront="1" vrnd="+"/>
	<vowel ph="_2" vlng="l" vheight="1" vfront="1" vrnd="+"/>
	<vowel ph="_3" vlng="l" vheight="1" vfront="1" vrnd="+"/>
	<vowel ph="_4" vlng="l" vheight="1" vfront="1" vrnd="+"/>
	<vowel ph="_5" vlng="l" vheight="1" vfront="1" vrnd="+"/>
	<vowel ph="_6" vlng="l" vheight="1" vfront="1" vrnd="+"/>
	<vowel ph="_7" vlng="l" vheight="1" vfront="1" vrnd="+"/>
	<vowel ph="_8" vlng="l" vheight="1" vfront="1" vrnd="+"/>

	<consonant ph="m" ctype="n" cplace="l" cvox="+"/>
	<consonant ph="n" ctype="n" cplace="d" cvox="+"/>
	<consonant ph="nj" ctype="n" cplace="p" cvox="+"/>
	<consonant ph="N" ctype="n" cplace="v" cvox="+"/>
	<consonant ph="b" ctype="s" cplace="l" cvox="+"/>
	

<!-- 	<consonant ph="P" ctype="s" cplace="l" cvox="-"/> -->
    <consonant ph="p" ctype="s" cplace="l" cvox="-"/>
	<consonant ph="d" ctype="s" cplace="l" cvox="+"/>

	<consonant ph="t" ctype="s" cplace="d" cvox="-"/>
	<consonant ph="tH" ctype="s" cplace="d" cvox="-"/>
	<consonant ph="tr" ctype="a" cplace="p" cvox="-"/>
	<consonant ph="c" ctype="a" cplace="p" cvox="-"/>
	<consonant ph="k" ctype="s" cplace="v" cvox="-"/>
    <consonant ph="Q" ctype="s" cplace="l" cvox="-"/>
	<consonant ph="v" ctype="f" cplace="b" cvox="+"/>
	<consonant ph="f" ctype="f" cplace="b" cvox="-"/>

	
	<consonant ph="z" ctype="f" cplace="a" cvox="+"/>
	<consonant ph="s" ctype="f" cplace="a" cvox="-"/>
	<consonant ph="zr" ctype="f" cplace="a" cvox="+"/>
	<consonant ph="sr" ctype="f" cplace="a" cvox="-"/>
